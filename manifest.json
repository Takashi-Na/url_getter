{
  "manifest_version": 3,
  "name": "URL Getter",
  "version": "1.0",
  "permissions": ["activeTab", "tabs"],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/wolf.png",
      "48": "images/wolf.png",
      "128": "images/wolf.png"
    }
  },
  "icons": {
    "16": "images/wolf.png",
    "48": "images/wolf.png",
    "128": "images/wolf.png"
  }
}
